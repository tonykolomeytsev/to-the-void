<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ru.bonsystems.tothevoid">

    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/MaterialParody_1"
        android:name="ru.bonsystems.tothevoid.platform.Controller">
        <meta-data android:name="com.google.android.gms.games.APP_ID" android:value="ru.bonsystems.miniplatform" />
        <activity
            android:name="ru.bonsystems.tothevoid.platform.MainActivity"
            android:alwaysRetainTaskState="true"
            android:screenOrientation="sensorLandscape"
            android:hardwareAccelerated="false">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
